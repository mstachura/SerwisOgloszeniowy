{% extends 'base.html.twig' %}

{% block title %}{{ 'rejestracja'|trans }}{% endblock %}

{% block body %}
    <h1>{{ 'lista_uzytkownikow'|trans }}</h1>

    {% include 'default/_paginator.html.twig' with {
        paginator: users,
        route_name: 'user_index',
    } %}

    {% for user in users.data %}
    <a href ="{{ url ('user_view', {'id': user.id}) }}">
        <button class="btn">
            {{ user.login}}
        </button>
    </a>
    {% endfor %}

{% endblock %}